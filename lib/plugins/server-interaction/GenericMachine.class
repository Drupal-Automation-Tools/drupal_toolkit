<?php

abstract class GenericMachine implements PhysicalServerInterface {
  
  private $vars;
  
  public function __construct(array $vars = array()) {
    $this->vars = $vars;
  }
  
  public function getSetting($key, $default = NULL) {
    if (isset($this->vars[$key])) {
      return $this->vars[$key];
    }
    return $default;
  }
  
}